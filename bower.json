{
  "name": "blueseed",
  "version": "0.0.1",
  "keywords": [
    "shopify",
    "theme",
    "framework",
    "css",
    "js",
    "sass",
    "mobile-first",
    "responsive",
    "front-end"
  ],
  "main": [
    "assets/*",
    "config/*",
    "layout/*",
    "snippets/*",
    "templates/*"
  ],
  "ignore": [
