    "node_modules",
    "bower_components"
  ],

  "description": "The Ultimate Shopify Theme Framework",
  "directory" : "assets/components",

  "dependencies": {
    "normalize-scss": "~3.0.1",
    "compass-mixins": "~0.12.3",
    "poxy": "latest",
    "jquery-pjax": "latest",
    "poxy-accordion": "~0.0.7"
  }
}
