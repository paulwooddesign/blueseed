////////////////////////////////////////////////////////////
// Master Copy Width
///////////////////////////////////////////////////////////
.copy-width,
section:before,
section:after {margin: 0 auto;}

$i: 4;
@while $i < 7 {
  .copy-width,
  section:before,
  section:after {

    @media (min-width:(($i * ($column + $gutter) - $gutter) / $em)) {
      width: ((($i - .5) * ($column + $gutter) - $gutter) / $em) !important;
		}
	}

  $i: $i + .5 !global;
}


$i: 7;
@while $i <  ($zone-a-max + 1) {

  .copy-width,
  section:before,
  section:after {
    @media (min-width:(($i * ($column + $gutter) - $gutter) / $em)) {
      width: ((($i - 2) * ($column + $gutter) - $gutter) / $em) !important;
	  }
  }

  $i: $i + 1 !global;
}




