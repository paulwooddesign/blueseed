//////////////////////////////////////////////////////////////////////
//Positioning
///////////////////////////////////////////////////////////////////
.oxy {
  @include translate(50%, -50%);
  position: absolute;
  right: 50%;
  top: 50%;
  z-index: 999;
}



.ox {
  @include translate(0%, -50%);
  left: 0;
  position: absolute;
  top: 50%;
  z-index: 999;

}

.ox_ {
  @include translate(0%, -50%);
  position: absolute;
  right: 0;
  top: 50%;
  z-index: 999;

}

.oy {
  position: absolute;
  right: 50%;
  top: 0;
  z-index: 999;
  @include translate(50%, 0%);
}

.oy_6 {
  @include translate(50%, 0%);
  position: absolute;
  right: 50%;
  top: 0;
  z-index: 999;
}


